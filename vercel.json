{
  "framework": "angular",
  "buildCommand": "npm run build",
  "outputDirectory": "dist/people-app",
  "rewrites": [
    { "source": "/favicon.ico", "destination": "/favicon.ico" },
    { "source": "/person/:match*", "destination": "https://crud-api-olive.vercel.app/person/:match*" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
